    <script src="https://cdn.staticfile.org/jquery-cookie/1.4.1/jquery.cookie.min.js"></script>
    <script>
        let getUrlString = location.href;
        let url = new URL(getUrlString);
        let url_cookie = url.search;
        $.cookie('url_cookie', url_cookie, { expires: 1, path: '/' });
    </script>

    <script>
        // 點選左上角logo不跳轉
        $('a[href="/"]').click(function (e) {
            e.preventDefault();
            return;
        });

        // 禮物只能點選一次
        let clicked = false;
        let dataset;
        $(".QAhover").on("click", function () {
            if (!clicked) {
                clicked = true;
                dataset = $(this).data("show");
                $("#Q1_s").attr("src", `https://lunanaturalbra.com/hk/lurphknew/img1216/gift/c${dataset}-2.png`);
                $(this).siblings().addClass("unclicked");
                $(this).removeClass("QAhover").siblings().removeClass("QAhover animate__animated");
                let windowheight = window.innerHeight;
                $('html , body').animate({
                    scrollTop: $('#Q1_s').offset().top - (0.2 * windowheight),
                }, 900);
            }
        });
        // 複製指定優惠碼
        $("#Q1_s").on("click", function () {
            var text = '';
            var textarea = document.getElementById("couponcode");
            // 復值
            if (dataset == "1") {
                text = 'L1002312X';
            } else if (dataset == "2") {
                text = 'L2312X120';
            } else if (dataset == "3") {
                text = '150L2312X';
            }
            textarea.innerText = text;
            // 選中
            // textarea.select();
            window.getSelection().selectAllChildren(textarea);
            // 複製
            document.execCommand('copy', true);
            // 移除输入框
            // document.body.removeChild(textarea);
            alert(`恭喜！已複製優惠折扣碼${text}！`);
        });

        $(".eventbtn").on("click", function () {
            if ($(window).scrollTop() > $('#offer').offset().top) {
                e.preventDefault()
            } else {
                $('html , body').animate({
                    scrollTop: $('#offer').offset().top,
                }, 900);
            }

        });
    </script>